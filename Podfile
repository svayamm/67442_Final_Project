# platform :ios, '9.0'

target 'Busy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Busy
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Google/SignIn'
  pod 'Alamofire', '~> 4.0'
  pod 'SwiftyJSON'
  pod 'ObjectMapper', '~> 2.2'

  target 'BusyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BusyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
